	langid = {english},
	file = {Hasan et al. - 2018 - Implementation of Fuzzy Logic in Autonomous Irriga.pdf:C\:\\Users\\Usuario\\Zotero\\storage\\3KC7CWYV\\Hasan et al. - 2018 - Implementation of Fuzzy Logic in Autonomous Irriga.pdf:application/pdf},
